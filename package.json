{
  "name": "reflexy",
  "version": "1.7.2",
  "description": "Flexbox layout react components",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && tsc --project ./tsconfig.json && webpack --config webpack.config.client.babel.js",
    "build-wp": "npm run clean && webpack --config webpack.config.client.babel.js",
    "prepublishOnly": "npm run clean && npm run build",
    "patch-publish": "npm version patch && npm publish && git push --follow-tags",
    "minor-publish": "npm version minor && npm publish && git push --follow-tags",
    "major-publish": "npm version major && npm publish && git push --follow-tags"
  },
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "contributors": [
    {
      "name": "Vladimir Zhukov",
      "url": "http://github.com/vlazh"
    }
  ],
  "homepage": "https://github.com/vlazh/reflexy",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlazh/reflexy"
  },
  "bugs": {
    "url": "https://github.com/vlazh/reflexy/issues"
  },
  "keywords": [
    "react",
    "flexbox",
    "flex",
    "layout"
  ],
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/css-modules": "^1.0.0",
    "@types/react": "^16.0.9",
    "autoprefixer": "^7.1.4",
    "awesome-typescript-loader": "^3.2.3",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "extract-loader": "^1.0.1",
    "file-loader": "^0.11.2",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.7.4",
    "prettier-eslint": "^8.2.0",
    "rimraf": "^2.6.2",
    "stylelint": "^8.1.1",
    "stylelint-config-css-modules": "^1.1.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.1.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.4.0",
    "tslint-consistent-codestyle": "^1.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.2.0",
    "typescript": "^2.5.3",
    "webpack": "^3.6.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^16.0.0"
  }
}
