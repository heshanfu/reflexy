{
  "name": "reflexy",
  "version": "1.12.5",
  "description": "Flexbox layout react components",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && tsc --project ./tsconfig.json && webpack --config webpack.client.config.babel.js",
    "build-ts": "npm run clean && tsc --project ./tsconfig.json",
    "build-wp": "npm run clean && webpack --config webpack.client.config.babel.js",
    "prepublishOnly": "npm run clean && npm run build",
    "patch-publish": "npm version patch && npm publish && git push --follow-tags",
    "minor-publish": "npm version minor && npm publish && git push --follow-tags",
    "major-publish": "npm version major && npm publish && git push --follow-tags"
  },
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "contributors": [
    {
      "name": "Vladimir Zhukov",
      "url": "http://github.com/vlazh"
    }
  ],
  "homepage": "https://github.com/vlazh/reflexy",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlazh/reflexy"
  },
  "bugs": {
    "url": "https://github.com/vlazh/reflexy/issues"
  },
  "keywords": [
    "react",
    "flexbox",
    "flex",
    "layout"
  ],
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/css-modules": "^1.0.0",
    "@types/react": "^16.0.25",
    "@vzh/configs": "^0.10.2",
    "autoprefixer": "^7.1.6",
    "awesome-typescript-loader": "^3.4.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "extract-loader": "^1.0.1",
    "file-loader": "^1.1.5",
    "postcss-loader": "^2.0.9",
    "rimraf": "^2.6.2",
    "stylelint": "^8.3.1",
    "stylelint-config-css-modules": "^1.1.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^2.1.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-consistent-codestyle": "^1.10.2",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.2",
    "webpack": "^3.9.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "react": ">= 15.x"
  }
}
